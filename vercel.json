{
  "version": 3,
  "functions": {
    "api/health": {
      "runtime": "nodejs22.x",
      "entrypoint": "backend/dist/index.js"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/health"
    },
    {
      "source": "/(.*)",
      "destination": "/admin/dist/index.html"
    }
  ]
}
